{
    "name": "lauchoit/laravel-hex-mod",
    "description": "Modular scaffolding generator for Laravel (Hexagonal Architecture)",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Lauchoit\\LaravelHexMod\\": "src/stubs/"
        },
        "classmap": ["src/"]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lauchoit\\LaravelHexMod\\LaravelHexModProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Jesus Laucho",
            "email": "contacto@lauchoit.com"
        }
    ],
    "minimum-stability": "rc",
    "prefer-stable": true,
    "require": {
        "php": "^8.1"
    }
}
